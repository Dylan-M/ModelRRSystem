all: pkgIndex.tcl

pkgIndex.tcl : $(GRPackages)
	echo "pkg_mkIndex . $(GRPackages)" | $(TCLSH)


	
clean:

install:

distclean: clean
	rm -f Makefile pkgIndex.tcl

