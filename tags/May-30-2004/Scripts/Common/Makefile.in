CommonScripts = StdMenuBar.tcl CTCPanel.tcl
TCLSH           = @TCLSH@
LATEX		= @LATEX@
DVIPS		= @DVIPS@
PS2PDF		= @PS2PDF@

all: pkgIndex.tcl Finn.pdf

pkgIndex.tcl : $(CommonScripts)
	echo "pkg_mkIndex . $(CommonScripts)" | $(TCLSH)

Finn.pdf: Finn.tex FinnFigure1.ps FinnFigure2a.ps FinnFigure2b.ps
	$(LATEX) Finn
	$(LATEX) Finn
	$(DVIPS) -t letter Finn -o
	$(PS2PDF) Finn.ps Finn.pdf

clean:
	rm -f Finn.aux Finn.log Finn.toc Finn.dvi

distclean: clean
	rm -f pkgIndex.tcl

